import { Injectable } from '@angular/core';
import { HttpClient } from '@angular/common/http';
import { Observable } from 'rxjs';

@Injectable({
  providedIn: 'root'
})
export class DerivadosService {
  private apiUrl = 'http://localhost:8080/api/derivados'; // ðŸ”¥ cÃ¡mbialo por tu URL real

  constructor(private http: HttpClient) {}

  guardarDerivados(payload: any): Observable<any> {
    return this.http.post<any>(`${this.apiUrl}/guardar`, payload);
  }
}
