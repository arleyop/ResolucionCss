this.tableNewDataSource = tableOriginalDataSourceClone.map(
  (group: { grupo: string; data: TableData[] }) => {
    const updatedData = group.data.map((item: TableData) => {
      if (item.Penumdoc == this.dataRowId) {
        return {
          ...item,
          Portfolio: element.cartera,
          Derivatives: element.derivados,
          NetExposure: element.exposicionNeta,
          Guarantees: element.garantia,
          TechnicalGuarantee: element.gtiaTecnica,
          FinancialGuarantee: element.gtiaFinanciera, // ðŸ‘€ aquÃ­ corregÃ­ porque estabas asignando gtiaTecnica dos veces
          Consumption: element.porcConsumo,
          TotalExposure: element.totalExposicion,
        };
      }
      return item;
    });

    return { ...group, data: updatedData };
  }
);
