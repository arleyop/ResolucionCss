export class SimulatorSimulatedCustomer {
    public gccgroupid!: string;
    /** Número de identificación */
    public penumdoc!: string;
    /** Nombre o Razón social  */
    public penomper!: string;
    /** Obliagciones de la cartera */
    public cartera!: number;
    /** Obligaiones de garantía financiera */
    public gtiaTecnica!: number;
    /** Obligaiones de garantía técnica */
    public gtiaFinanciera!: number;
    /** Olbigaciones de derivados */
    public derivados!: number;
    /** suma de los creditos agrupados por grupo/clientes */
    public totalExposicion!: number;
    /** Provisiones del grupo/cliente */
    public provisiones!: number;
    /** Garantías del grupo/cliente */
    public garantia!: number;
    /** Expsocion por grupo/cliente calculada */
    public exposicionNeta!: number;
    /** % que arroja el calculo final de la expsocion */
    public porcConsumo!: number; 
}
