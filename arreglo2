    createSimulation(simulatorRequest: SimulatorRequest): Observable<SimulatorSimulatedCustomer[]> {
        let url = `${this.endpoint}/ejecutar-simulador`;
        let observable: Observable<any> = new Observable<any>(
            (subscriber: Subscriber<any>) => {
                this.http.post<JSend>(url, simulatorRequest).subscribe({
                    next: (response: JSend) => {
                        // Si el estado de la respuesta (JSend) es "success"
                        if (response.status === 'success') {
                            if (response.data.length === 0) {
                                subscriber.error('No se encontraron datos de simulaciÃ³n');
                            }
                            let simulatorSimulatedCustomer: SimulatorSimulatedCustomer = response.data[0];
                            subscriber.next(simulatorSimulatedCustomer);
                        }
                        // Si el estado de la respuesta (JSend) es "fail" o "error"
                        else {
                            subscriber.error(response.message);
                        }
                    },
                    error: (error: any) => { subscriber.error(error.error.message) },
                    complete: () => { subscriber.complete() }
                });
            }
        );
        return observable;
    }
