let component: RatingComponent;
let fixture: ComponentFixture<RatingComponent>;
let iconRegistry: MatIconRegistry;
let sanitizer: DomSanitizer;
let iconService: IconService;

beforeEach(waitForAsync(() => {
  TestBed.configureTestingModule({
    providers: [
      provideHttpClient(),
      provideHttpClientTesting(),
      IconService,
    ],
    imports: [
      MatIconModule,
      // Add other required modules here
    ]
  }).compileComponents();
}));

beforeEach(() => {
  iconRegistry = TestBed.inject(MatIconRegistry);
  sanitizer = TestBed.inject(DomSanitizer);
  
  // Configure icons
  const commonIcon = ["close", "search", "menu", "add", "delete"];
  commonIcon.forEach(iconName => {
    iconRegistry.addSvgIconLiteral(iconName, sanitizer.bypassSecurityTrustHtml('<svg></svg>'));
  });
  
  fixture = TestBed.createComponent(RatingComponent);
  component = fixture.componentInstance;
  iconService = TestBed.inject(IconService);
  fixture.detectChanges();
});

it('should create', () => {
  expect(component).toBeTruthy();
});
